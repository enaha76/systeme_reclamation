<script>
    array_name=[]
    $('table tbody tr').each(function() {
        // Get the text content of the first cell in this row
        var name_array = $(this).find('td:first-child').text().trim();
        
        array_name.push(name_array)
        
        // Log the subject name to the console
    
    });


    
    function findMissingElement(arr1, arr2) {
        const missingElements = [];
    
        for (let i = 0; i < arr1.length; i++) {
        if (!arr2.includes(arr1[i])) {
            missingElements.push(arr1[i]);
        }
        }
    
        for (let i = 0; i < arr2.length; i++) {
        if (!arr1.includes(arr2[i])) {
            missingElements.push(arr2[i]);
        }
        }
    
        return missingElements;
    }
    function isempty(arr) {
        if (arr.length === 0) {
            return true;
        } else {
            return false;
        }
    }

    function counter(arr) {
        let ctr=0;
        for (const key in arr) {
            
            if (isempty(arr[key])==false){
                ctr++;
            }
        }
        return ctr;
    }


    $(document).ready(function() {
        // Create an empty object to store the arrays
        let subjectArrays = {};
    
        // Add change event listener to all checkboxes with class "subject"
        $(".subject").change(function() {
          // Get the name of the subject from the first table cell
        let subjectName = $(this).closest("tr").find("td:first-child").text().trim();
    
          // Create an array for this subject if it doesn't already exist
        if (!subjectArrays[subjectName]) {
            subjectArrays[subjectName] = [];
        }
    
          // If the checkbox is checked, add its value to the subject's array
        if ($(this).prop("checked")) {
            subjectArrays[subjectName].push($(this).data("type"));
        } else {
            // If the checkbox is unchecked, remove its value from the subject's array
            var dataType = $(this).data("type");
            subjectArrays[subjectName] = subjectArrays[subjectName].filter(function(val) {
                return val !== dataType;
            });
        }
        
        
        //console.log(subjectArrays);
          // Log the updated subject arrays to the console
       // console.log( Object.keys(subjectArrays))
        window.m={}
        window.m=subjectArrays;

      //  console.log( Object.keys(subjectArrays).length)
        const jsonString = JSON.stringify(subjectArrays);
        let b=findMissingElement(array_name,Object.keys(subjectArrays))
        //console.log("huuuuuuuuuuuuuuuuun",b,"allowed")
        document.getElementById('ane').value=jsonString;
    
        if(counter(subjectArrays) >= 3){ 
            $("#max-subjects-message").show();

            $("tr td:first-child").each(function() {
            //console.log("fksjhbhsd")
            for (var i=0; i<b.length; i++){
                if ($(this).text().indexOf(b[i]) === 0) {
                $(this).parent("tr").hide();
                }}
            });
            
        }
        else {
            $("#max-subjects-message").hide();
            $("tr td:first-child").each(function() {
                //console.log("fksjhbhsd")
                for (var i=0; i<array_name.length; i++){
                    if ($(this).text().indexOf(array_name[i]) === 0) {
                    $(this).parent("tr").show();
                    }}
                });

        }

        });
    });
    

    
    

</script>





