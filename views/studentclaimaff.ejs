                                                                            <td>
                                                                                <ul class="list-group">
                                                                                    
                                                                                    <% Object.entries(document.matiere).forEach(function([key, value]) { %>
                                                                                    <li
                                                                                        class="list-group-item d-flex justify-content-between align-items-center">
                                                                                        <%= key %>
                                                                                    
                                                                                        <% if (value.includes('tp')){ %>
                                                                                        <span
                                                                                            class="badge bg-primary rounded-pill">TP</span>
                                                                                        <% } %>

                                                                                        <% if (value.includes('devoir')){ %>
                                                                                            <span class="badge bg-success rounded-pill">Devoir</span>
                                                                                            <% } %>

                                                                                        <% if (value.includes('examen')){ %>
                                                                                            <span class="badge bg-danger rounded-pill">Examen</span>
                                                                                        <% } %>
                                                                                    </li>
                                                                                    <% }) %>
                                                                                
                                                                                </ul>
                                                                            </td>
                                                                            
                                                                            




                                                                            <td
                                                                                style="word-wrap: break-word; max-width: 300px; font-weight: bold;">
                                                                                <div class="overflow-auto">
                                                                                    <p>
